{
  "name": "lost-in-a-postcard",
  "version": "0.1.0",
  "description": "Lost In A Postcard. Travel photo blog by Domi & Matt.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "babel server -d dist",
    "client": "npm run start --prefix client",
    "server": "nodemon --exec babel-node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "preinstall": "npx npm-force-resolutions",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client && npm run build"
  },
  "author": "Mateusz Pyzowski",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^2.9.15",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.18.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-upload": "^9.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.1",
    "mongoose-paginate": "^5.0.3",
    "mongoose-unique-validator": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "concurrently": "^5.0.1",
    "nodemon": "^2.0.2"
  },
  "resolutions": {
    "fs-capacitor": "5.0.1"
  }
}
