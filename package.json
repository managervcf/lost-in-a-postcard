{
  "name": "lost-in-a-postcard",
  "version": "0.2.0",
  "description": "Lost in a Postcard. Travel photography portfolio by Domi & Matt.",
  "repository": {
    "type": "git",
    "url": "https://github.com/managervcf/lost-in-a-postcard"
  },
  "author": {
    "name": "Mateusz Pyzowski",
    "email": "managervcf@gmail.com",
    "url": "https://github.com/managervcf"
  },
  "license": "MIT",
  "engines": {
    "node": "13.14.0",
    "npm": "6.14.7"
  },
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/dist/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm run build:client && npm run build:server",
    "build:client": "npm install --prefix src/client && npm run build --prefix src/client",
    "build:server": "babel src/server -d src/dist",
    "client": "npm run start --prefix src/client",
    "server": "npm run build:server && nodemon src/dist/index.js",
    "heroku-postbuild": "npm run build",
    "test": "jest --runInBand --watchAll",
    "test:ci": "jest --runInBand --ci"
  },
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "apollo-server-express": "^2.17.0",
    "aws-sdk": "^2.761.0",
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.4",
    "mongoose-paginate-v2": "^1.3.9",
    "mongoose-unique-validator": "^2.0.3",
    "regenerator-runtime": "^0.13.7",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.4",
    "puppeteer": "^5.2.1"
  }
}
