{
  "name": "lost-in-a-postcard",
  "version": "0.1.0",
  "description": "Lost in a Postcard. Travel photography portfolio by Domi & Matt.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:prod\" \"npm run cache\"",
    "start:prod": "node dist/index.js",
    "build": "babel server -d dist",
    "cache": "redis-server",
    "client": "npm run start --prefix client",
    "server": "npm run build && nodemon dist/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run cache\"",
    "preinstall": "npx npm-force-resolutions",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client && npm run build"
  },
  "author": "Mateusz Pyzowski",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^2.14.2",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.21.0",
    "concurrently": "^5.2.0",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-upload": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.12",
    "mongoose-paginate-v2": "^1.3.9",
    "mongoose-unique-validator": "^2.0.3",
    "redis": "^3.0.2",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "nodemon": "^2.0.3"
  },
  "resolutions": {
    "fs-capacitor": "5.0.1"
  }
}
